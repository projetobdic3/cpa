<%= form_for(@questionario) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="field">
    <%= f.label :nome %><br>
    <%= f.text_field :nome %>
  </div>

  <div class="field">
    <%= f.label :previsao_termino, "Previsão de Término" %><br>
    <%= f.date_select :previsao_termino %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
